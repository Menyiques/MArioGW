sub init_sprites()
	dim addr as uinteger
	addr=@sprite+7
	dim n as ubyte
	dim ancho, alto as uinteger
	'Cambiar este for por un array de sprites con sus direcciones'
	addr_sprite(0)=addr
	for n=1 to 29
		ancho=peek (addr)
		alto=peek(addr+1)
		addr=addr+ancho*alto*9+2:'8 bytes + 1 de atrubutos'
		addr_sprite(n)=addr
	next n
end sub


sub pinta_caja( n as ubyte, modo as ubyte)
	dim ss as ubyte
	dim cajax, cajay as ubyte
	poke uinteger 23675,@udg
	cajax=poscajas(n,0)
	cajay=poscajas(n,1)+1
	a$="\A"
	ss=n
	ss=(ss+1) / 8
	if ss=0 then a$="\A\A"
	if ss=1 then a$="\B\A"
	if ss=2 then a$="\B\B"
	if ss=3 then a$="\C\B"
	if ss=4 then a$="\C\C"	
	if ss>4 then a$="\D\E"
	
	if modo=0 
		a$="  "
	end if
	
	if not(n>1 and n<>6 and n<>14 and n<>22 and n<>30 and n<>38)  
		a$=a$(0 to 0)
	end if

	print at cajay,cajax;ink 8;paper 8;bright 8;a$

end sub



