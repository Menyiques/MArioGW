sub tile( nsprite as ubyte)
	tilexy(coords(nsprite ,0),coords(nsprite ,1),nsprite )
end sub

sub tilexy(x as ubyte, y as ubyte,nsprite as ubyte)
	tilexyaddr (x,y,nsprite,$40,@sprite)
end sub


sub tilexyaddr(x as ubyte, y as ubyte,nsprite as ubyte,mem as ubyte,sprites as uinteger)
	poke @sprite,x:'c
	poke @sprite+1,y:'b
	dim addr as uinteger
	addr=addr_sprite(nsprite)
	dim lx,ly as ubyte
	lx=peek (addr)
	ly=peek (addr+1)
	addr=addr+2
	poke @sprite+2,lx
	poke @sprite+3,ly
	poke uinteger @sprite+4,addr
	poke @sprite+6,mem
	tylexyasm(@sprite)
end sub

